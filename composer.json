{
    "name": "alexander-fuchs/hashidable",
    "description": "Auto Hashids for Laravel Models and Routes",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Samuel Oloruntoba",
            "email": "tag@kayandra.co"
        },
        {
            "name": "Alexander Fuchs",
            "email": "af@altinum.tech"
        }
    ],
    "require": {
        "php": ">=8.0",
        "hashids/hashids": "^5.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.5",
        "phpunit/phpunit": "^10.0",
        "orchestra/database": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Altinum\\Hashidable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Altinum\\Hashidable\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Altinum\\Hashidable\\HashidableServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit --color=always"
    },
    "minimum-stability": "stable"
}
